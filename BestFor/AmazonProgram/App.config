<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
  </appSettings>
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    
    <extensions>
      <behaviorExtensions>
        <add name="randomNameA" type="AmazonProduct.WCF.AmazonSigningBehaviorExtensionElement, AmazonProduct.WCF, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null"/>
      </behaviorExtensions>
    </extensions>
    <behaviors>
      <endpointBehaviors>
        <behavior name="randomNameB">
       <!--   <randomNameA AmazonAccessKeyId="03W6RP0TG4BPK9P84S02" AmazonSecretKey="WG6rXo98GWQtw4P4U3FU0Tx8Ve3T4IXJJ7t39Hjx"
                       AmazonSecurityNamespace="http://security.amazonaws.com/doc/2007-01-01/"/> -->

                    <randomNameA AmazonAccessKeyId="AKIAI73IO4NBBOWQJ33Q" AmazonSecretKey="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"
                       AmazonSecurityNamespace="http://security.amazonaws.com/doc/2007-01-01/"/>
        </behavior>
      </endpointBehaviors>
    </behaviors>
    
    <bindings>
      <basicHttpBinding>
        <binding name="AWSECommerceServiceBinding" allowCookies="true"
                 maxReceivedMessageSize="20000000"
                 maxBufferSize="20000000"
                 maxBufferPoolSize="20000000">
          <readerQuotas maxDepth="32"
                 maxArrayLength="200000000"
                 maxStringContentLength="200000000"/>
          <security mode="Transport" />
        </binding>

      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="https://webservices.amazon.com/onca/soap?Service=AWSECommerceService"
          binding="basicHttpBinding" bindingConfiguration="AWSECommerceServiceBinding"
          contract="Amazon.APAPI.AWSECommerceServicePortType" name="AWSECommerceServicePort"
          behaviorConfiguration="randomNameB"/>
 
    </client>
  </system.serviceModel>
</configuration>